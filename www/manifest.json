{
  "_version": "1.8.0",
  "sap.app": {
    "id": "pae.logistica",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "invoiceRemote": {
        "uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "pae.logistica.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.30",
      "libs": {
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "pae.logistica.i18n.i18n"
        }
      },
      "reservas": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "mock/Reservas.json"
      },
      "invoice": {
        "dataSource" : "invoiceRemote"
      },
      "invoice2": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "Invoices.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "pae.logistica.controller.CustomRouter",
        "viewType": "XML",
        "viewPath": "pae.logistica.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
					"target": "NotFound"
				},
        "async": true
      },
      "routes": [
        {
          "pattern": "App",
          "name": "App",
          "target": "App",
          "subroutes": [
            {
              "pattern": "Menu",
              "name" : "Menu",
              "target" : "Menu",
              "subroutes": [
                {
                  "pattern": "Home",
                  "name": "Home",
                  "target": "Home",
                  "subroutes": [
                    {
                      "pattern": "EntregaEntrante/{OrderID}",
                      "name": "EntregaEntrante",
                      "target": "EntregaEntrante"
                    }
                  ]
                },
                {
                  "pattern": "Salidas",
                  "name": "Salidas",
                  "target": "Salidas",
                  "subroutes":[
                    {
                      "pattern": "Reserva",
                      "name": "Reserva",
                      "target": "Reserva",
                      "subroutes": [
                        {
                          "pattern": "Escanear",
                          "name": "Escanear",
                          "target": "Escanear"
                        },
                        {
                          "pattern": "Ingresar",
                          "name": "Ingresar",
                          "target": "Ingresar"
                        },
                        {
                          "pattern": "Listado",
                          "name": "Listado",
                          "target": "Listado"
                        }
                      ]
                    },
                    {
                      "pattern": "Entrega",
                      "name": "Entrega",
                      "target": "Entrega"
                    },
                    {
                      "pattern": "SinReferencia",
                      "name": "SinReferencia",
                      "target": "SinReferencia"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ],
      "targets": {
        "Escanear": {
          "viewId": "Escanear",
          "viewName": "menu.salidas.reserva.escanear.Escanear",
          "viewLevel": 5
        },
        "Ingresar": {
          "viewId": "Ingresar",
          "viewName": "menu.salidas.reserva.ingresar.Ingresar",
          "viewLevel": 5
        },
        "Listado": {
					"viewId": "Listado",
          "viewName": "menu.salidas.reserva.listado.Listado",
          "viewLevel": 5
        },
        "SinReferencia": {
					"viewId": "SinReferencia",
          "viewName": "menu.salidas.sinreferencia.SinReferencia",
          "viewLevel": 4
        },
        "Entrega": {
					"viewId": "Entrega",
          "viewName": "menu.salidas.entrega.Entrega",
          "viewLevel": 4
				},
        "NotFound": {
					"viewId": "NotFound",
          "viewName": "NotFound"
				},
        "Home": {
          "viewId": "Home",
          "viewName": "menu.recepcion.Home",
          "viewLevel": 3
        },
        "Salidas": {
          "viewId": "Salidas",
          "viewName": "menu.salidas.Salidas",
          "viewLevel": 3
        },
        "Reserva": {
          "viewId": "Reserva",
          "viewName": "menu.salidas.reserva.Reserva",
          "viewLevel": 4
        },
        "Menu": {
          "viewId": "Menu",
          "viewName": "menu.Menu",
          "viewLevel": 2
        },
        "App": {
          "viewId": "App",
          "viewName": "App",
          "ViewLevel": 1
        },
        "Avisos": {
          "viewId": "EntregaEntrante",
          "ViewName": "EntregaEntrante",
          "ViewLevel": 4
        }
      }
    }
  }
}